import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        double fl = in.nextDouble();
        int n1=in.nextInt();
        int n2 = in.nextInt();
        int n3 = in.nextInt();
        System.out.println((int)Math.floor(fl));
        System.out.println((int)Math.ceil(fl));
        System.out.println((int)Math.sqrt(n1));
        System.out.println((int)Math.pow(n2,n3));
    }
}
